			<Parameter Name="PayName1" Caption="Тип безнал. оплаты 1" Description="Тип безнал. оплаты 1" TypeValue="String" ReadOnly="False" DefaultValue="ПЛАТ.КАРТОЙ"/>
			<Parameter Name="PayName2" Caption="Тип безнал. оплаты 2" Description="Тип безнал. оплаты 2" TypeValue="String" ReadOnly="False" DefaultValue="КРЕДИТОМ"/>
			<Parameter Name="PayName3" Caption="Тип безнал. оплаты 3" Description="Тип безнал. оплаты 3" TypeValue="String" ReadOnly="False" DefaultValue="СЕРТИФИКАТОМ"/>
		</Group>
	</Page>
	<Page Caption="Настройка лога">
		<Group Caption="Настройка лога">
			<Parameter Name="EnableLog" Caption="Вести лог (путь к логу настраивается через тест драйвера)" Description="Вести лог (путь к логу настраивается через тест драйвера)" TypeValue="Boolean" ReadOnly="False" DefaultValue="False"/>
			<Parameter Name="LogPath" Caption="Путь к файлу лога" Description="Путь к файлу лога" TypeValue="String" ReadOnly="False"/>
		</Group>
	</Page>
	<Page Caption="Формат чека">
		<Group Caption="Формат чека">
			<Parameter Name="DisablePrintReports" Caption="Не печатать фискальные отчеты и чеки внесения/выемки на бумаге" Description="Не печатать фискальные отчеты и чеки внесения/выемки на бумаге" TypeValue="Boolean" ReadOnly="False" DefaultValue="False"/>
			<Parameter Name="EnablePaymentSignPrint" Caption="Печатать признак и способ расчета (ФФД 1.05)" Description="Печатать признак и способ расчета (ФФД 1.05)" TypeValue="Boolean" ReadOnly="False" DefaultValue="True"/>
			<Parameter Name="ItemNameLength" Caption="Кол-во используемых символов наименования товара (0 - использовать целиком)" Description="Кол-во используемых символов наименования товара (0 - использовать целиком)" TypeValue="Number" ReadOnly="False" DefaultValue="0"/>
			<Parameter Name="CheckFontNumber" Caption="Номер шрифта тела чека" Description="Номер шрифта тела чека" TypeValue="Number" ReadOnly="False" DefaultValue="1"/>
			<Parameter Name="EnableNonFiscalHeader" Caption="Печатать заголовок нефиск. документа" Description="Печатать заголовок нефиск. документа" TypeValue="Boolean" ReadOnly="False" DefaultValue="True"/>
			<Parameter Name="EnableNonFiscalFooter" Caption="Печатать окончание нефиск. документа" Description="Печатать окончание нефиск. документа" TypeValue="Boolean" ReadOnly="False" DefaultValue="True"/>
			<Parameter Name="UseRepeatDocument" Caption="Для печати копии последнего документа использовать команду повтора документа" Description="Для печати копии последнего документа использовать команду повтора документа" TypeValue="Boolean" ReadOnly="False" DefaultValue="False"/>
		</Group>
	</Page>
